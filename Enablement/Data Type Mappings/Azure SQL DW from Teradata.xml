<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<data_type_mapping_set name="Azure SQL DW from Teradata" xsi:schemaLocation="http://www.wherescape.com/xml/red ./Data%20Type%20Mapping.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.wherescape.com/xml/red">
	<mapping_set_description></mapping_set_description>
	<mapping_set_null_policy>AllowNulls</mapping_set_null_policy>
	<data_type_mapping match_data_type="date" output_data_type="date">
		<transform_code>TO_CHAR(%column_name,'YYYY-MM-DD')</transform_code>
	</data_type_mapping>
	<data_type_mapping match_data_type="timestamp" output_data_type="datetime">
		<transform_code>TO_CHAR(%column_name,'YYYY-MM-DD HH24:MI:SS')</transform_code>
	</data_type_mapping>
	<data_type_mapping match_data_type="time" output_data_type="time">
		<transform_code>TO_CHAR(%column_name,'HH24:MI:SS')</transform_code>
	</data_type_mapping>
	<data_type_mapping match_data_type="char" output_data_type="varchar(%precision)">
		<match_parameters>
			<match_precision>1-8000</match_precision>
		</match_parameters>
		<transform_code>RTRIM(%column_name)</transform_code>
	</data_type_mapping>
	<data_type_mapping match_data_type="char" output_data_type="varchar(8000)">
		<match_parameters>
			<match_precision>8001-*</match_precision>
		</match_parameters>
		<transform_code>SUBSTRING(RTRIM(%column_name) FROM 1 FOR 8000)</transform_code>
	</data_type_mapping>
	<data_type_mapping match_data_type="varchar" output_data_type="varchar(%precision)">
		<match_parameters>
			<match_precision>1-8000</match_precision>
		</match_parameters>
	</data_type_mapping>
	<data_type_mapping match_data_type="varchar" output_data_type="varchar(8000)">
		<match_parameters>
			<match_precision>8001-*</match_precision>
		</match_parameters>
		<transform_code>SUBSTRING(%column_name FROM 1 FOR 8000)</transform_code>
	</data_type_mapping>
	<data_type_mapping match_data_type="float" output_data_type="float"></data_type_mapping>
	<data_type_mapping match_data_type="byteint" output_data_type="tinyint"></data_type_mapping>
	<data_type_mapping match_data_type="smallint" output_data_type="smallint"></data_type_mapping>
	<data_type_mapping match_data_type="integer" output_data_type="integer"></data_type_mapping>
	<data_type_mapping match_data_type="bigint" output_data_type="bigint"></data_type_mapping>
	<data_type_mapping match_data_type="clob" output_data_type="varchar(8000)">
		<transform_code>SUBSTRING(%column_name FROM 1 FOR 8000)</transform_code>
	</data_type_mapping>
	<data_type_mapping match_data_type="blob" output_data_type="varbinary(8000)"></data_type_mapping>
</data_type_mapping_set>
